<%- include("_header") %>
<% if (currentUser) { %>
    <a href="/posts/create" class="btn btn-primary link-btn">Create Post</a>
    <a href="/users/logout" class="btn btn-primary link-btn m-3">Log Out</a> 
<% } %>
<% if (posts.length > 0) { %>
    <% posts.forEach(function(post) { %>
        <%- include("_postcard", { post: post }) %>
    <% }) %>
<% } %>
<%- include("_membership_modal_form") %> 
<%- include("_footer") %>